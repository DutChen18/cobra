server {
	listen 127.0.0.1:80
	error_page 404 /error/404.html

	set_header server cobra
	index index.html

	#ssl cert.pem key.pem
	
	location / {
		#set_header cache-control max-age=14400
		root /home/codam/cobra
		static true
	}

	location /error {
		root /home/codam/cobra/config/error
		static false
	}

	root /

	max_body_size 1000
	location / {
		root /home/codem/cobra
	}
}
